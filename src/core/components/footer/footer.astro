---
import "./footer.scss";

const navList = [
  { name: "HOME", navTo: "/" },
  { name: "WORK", navTo: "/works" },
  { name: "CONTACT", navTo: "#contact" },
];
---

<footer class="footer">
  <div class="footer__container">
    <nav class="footer__nav">
      <ul class="footer__items">
        {
          navList.map((item) => (
            <li>
              <a href={item.navTo}>{item.name}</a>
            </li>
          ))
        }
      </ul>
    </nav>
  </div>
</footer>
